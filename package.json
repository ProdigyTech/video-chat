{
  "name": "web-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev-server:build": "next build",
    "dev-server:rtc": "nohup peerjs --port 3001 > /dev/null 2>&1 &",
    "dev-server:server": "node video-server.js",
    "dev-server": "run-s dev-server:*",
    "prod:build": "next build",
    "prod:rtc": "nohup peerjs --port 3001 --proxied true --path /peer > /dev/null 2>&1 &",
    "prod:server": "NODE_ENV=production node video-server.js",
    "prod": "run-s prod:*"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.2",
    "babel-runtime": "^6.26.0",
    "express": "^4.17.1",
    "next": "10.0.4",
    "nohup": "^0.1.0",
    "npm-run-all": "^4.1.5",
    "peer": "^0.6.1",
    "peerjs": "^1.3.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "uuidv4": "^6.2.6",
    "node-sass": "^4.14.1"
  }
}
